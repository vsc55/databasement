# MySQL CLI Type (mariadb or mysql)
# Use 'mariadb' for local development with MariaDB CLI tools (mariadb-dump, mariadb-restore)
# Use 'mysql' for environments with MySQL CLI tools (mysqldump, mysql)
MYSQL_CLI_TYPE=mariadb

# ============================================
# OAuth Configuration
# ============================================

# Allow new users to register via OAuth (default: true)
OAUTH_AUTO_CREATE_USERS=true

# Default role for new OAuth users (viewer, member, admin)
OAUTH_DEFAULT_ROLE=member

# Auto-link OAuth users to existing accounts by email (default: true)
OAUTH_AUTO_LINK_BY_EMAIL=true

# Remember OAuth users (default: true)
OAUTH_REMEMBER_ME=true

# --------------------------------------------
# Google OAuth
# --------------------------------------------
OAUTH_GOOGLE_ENABLED=false
OAUTH_GOOGLE_CLIENT_ID=
OAUTH_GOOGLE_CLIENT_SECRET=

# --------------------------------------------
# GitHub OAuth
# --------------------------------------------
OAUTH_GITHUB_ENABLED=false
OAUTH_GITHUB_CLIENT_ID=
OAUTH_GITHUB_CLIENT_SECRET=

# --------------------------------------------
# GitLab OAuth
# --------------------------------------------
OAUTH_GITLAB_ENABLED=false
OAUTH_GITLAB_CLIENT_ID=
OAUTH_GITLAB_CLIENT_SECRET=
OAUTH_GITLAB_HOST=https://gitlab.com

# --------------------------------------------
# Generic OIDC (Dex, Keycloak, Authentik, etc.)
# --------------------------------------------
OAUTH_OIDC_ENABLED=false
OAUTH_OIDC_CLIENT_ID=
OAUTH_OIDC_CLIENT_SECRET=
OAUTH_OIDC_BASE_URL=
OAUTH_OIDC_LABEL=SSO
