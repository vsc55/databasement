{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/livewire-starter-kit",
    "type": "project",
    "description": "The official Laravel starter kit for Livewire.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "ext-ftp": "*",
        "ext-pdo": "*",
        "blade-ui-kit/blade-icons": "^1.8",
        "codeat3/blade-devicons": "^1.8",
        "dedoc/scramble": "^0.13.8",
        "kovah/laravel-socialite-oidc": "^0.5.0",
        "laravel/fortify": "^1.30",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.2",
        "laravel/socialite": "^5.24",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-ftp": "^3.31",
        "league/flysystem-sftp-v3": "^3.31",
        "livewire/volt": "^1.7.0",
        "owenvoke/blade-fontawesome": "^2.7",
        "robsontenorio/mary": "^2.4",
        "spatie/laravel-query-builder": "^6.3"
    },
    "require-dev": {
        "ext-zlib": "*",
        "barryvdh/laravel-ide-helper": "^3.6",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.8",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/phpstan": "^2.1",
        "rector/rector": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php artisan migrate --force"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "audit": {
            "ignore": ["PKSA-365x-2zjk-pt47"]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
